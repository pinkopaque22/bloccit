<h1><%= @question.title %></h1>
<p><%= @question.body %></p>
<p><%= @question.resolved %></p>
<div class="col-md-4">
    <%= link_to "Edit", edit_question_path(@question), class: 'btn btn-success' %>
    <%= link_to "Delete Question", @question, method: :delete, class: 'btn btn-danger' %>
    data: { confirm: 'Are You Sure You Want Me To Delete This Question?' }
</div>
